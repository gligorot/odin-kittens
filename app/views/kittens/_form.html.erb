<%= form_for(@kitten) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<br>
	<%= f.label :name %>
	<%= f.text_field :name, placeholder: "Name" %>
	<br>
	<%= f.label :age %>
	<%= f.number_field :age %>
	<br>
	<%= f.label :cuteness %>
	<%= f.number_field :cuteness, in: 1..10 %>
	<br>
	<%= f.label :softness %>
	<%= f.number_field :softness, in: 1..10 %>
	<br>
	<%= f.submit yield(:button_text) %>
<% end %>